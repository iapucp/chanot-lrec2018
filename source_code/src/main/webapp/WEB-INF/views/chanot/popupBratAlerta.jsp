<div id="modalBratAlerta" class="modal fade" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <div class="panel panel-primary">
                    <div class="panel-heading text-left modal-title">
                        <strong>Exportar a brat</strong>
                    </div>
                </div>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group row">
                            <label class="col-md-10 col-form-label text-right" id="mensajeBrat"></label>                               
                        </div>                           
                    </div>
                </div> 
            </div>

            <div class="modal-footer">
                <div class="row">
                    <div class="col-md-12 pull-right">
                        <div class="pull-right">
                            <button type="button" class="btn btn-info btn-sm" id="btnPopupBratAlertaContinuar">
                                <span class="glyphicon glyphicon-check"></span>
                                <span class="js-label">Continuar</span>
                            </button>
                            <button type="button" class="btn btn-danger btn-sm" id="btnPopupBratAlertaCancelar">
                                <span class="glyphicon glyphicon-remove"></span>
                                <span class="js-label">Cancelar</span>
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
