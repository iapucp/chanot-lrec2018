<div id="modalBrat" class="modal fade" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <div class="panel panel-primary">
                    <div class="panel-heading text-left modal-title">
                        <strong>Exportar a brat</strong>
                    </div>
                </div>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group row">
                            <label class="col-md-3 col-form-label text-left"></label>
                            <label class="col-md-5 col-form-label text-right">Seleccione su lugar de trabajo</label>                               
                        </div>                           
                    </div>
                </div> 
            </div>

            <div class="modal-footer">
                <div class="row">
                    <div class="col-md-12 pull-right">
                        <div class="pull-right">
                            <button type="button" class="btn btn-info btn-sm" id="btnPucp">
                                <span class="glyphicon glyphicon-export"></span>
                                <span class="js-label">PUCP</span>
                            </button>
                            <button type="button" class="btn btn-info btn-sm" id="btnPutty">
                                <span class="glyphicon glyphicon-export"></span>
                                <span class="js-label">Via Putty</span>
                            </button>
                            <button type="button" class="btn btn-danger btn-sm" id="btnPopupBratCancelar">
                                <span class="glyphicon glyphicon-remove"></span>
                                <span class="js-label">Cancelar</span>
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
